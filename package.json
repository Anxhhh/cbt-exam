{
  "name": "cbt-exam",
  "homepage": "https://anxhhh.github.io/cbt-exam",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "predeploy": "npm run build",
    "start": "npm run build && npm run electron:start",
    "dev": "concurrently -k \"vite\" \"npm run electron:dev\"",
    "build": "vite build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "electron:dev": "cross-env IS_DEV=true electron-forge start",
    "electron:start": "electron .",
    "electron:build": "electron-forge make"
  },
  "dependencies": {
    "csv-parser": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "electron": "^30.0.0",
    "gh-pages": "^6.3.0",
    "vite": "^5.0.0"
  }
}
